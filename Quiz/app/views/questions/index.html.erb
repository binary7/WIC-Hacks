<div class="wrapper">
  <div id="main">
    <div class="title">
      <h1>Career Quiz</h1>
    </div>
     <div class="containerPlain">
       Answer each question with the value that best describes you.
    </div>
      <p id="notice"><%= notice %></p>
      <%= form_tag do %>
        <% @questions.each do |question| %>
      <div class="container">
      <p>
        <%= question.body %>
      </p>
        <select>
        <% question.answers.each do |answer| %>
        <option value="<%= answer.value %>"> <%= answer.text %> </option>
        <% end %>
        </div>
        <% end %>
       <div class="containerPlain">
        <%= submit_tag %>
        </div>
      <% end %>
  </div>
</div>

